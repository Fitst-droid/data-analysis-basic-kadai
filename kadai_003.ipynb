        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1],[2, 3],[4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "# 行列積\n",
        "AB = np.dot(A, B)\n",
        "print(AB)\n",
        "# 行列の最大値\n",
        "max_AB = np.max(AB)\n",
        "print(max_AB)"
